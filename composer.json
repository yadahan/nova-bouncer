{
    "name": "yadahan/nova-bouncer",
    "description": "A Laravel Nova tool for managing Bouncer roles and abilities.",
    "keywords": [
        "laravel",
        "nova",
        "bouncer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yaakov Dahan",
            "email": "yakidehan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0|^5.0",
        "silber/bouncer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yadahan\\BouncerTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yadahan\\BouncerTool\\BouncerToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
